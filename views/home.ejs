    <section class="section dashboard">
      <div class="row">

        <!-- Left side columns -->
        <div class="col-lg-8">
          <div class="row">

            <!-- Nhịp tim  -->
            <div class="col-xxl-4 col-md-6">
              <div class="card info-card revenue-card">

                <div class="card-body">
                  <h5 class="card-title">Nhịp Tim</h5>

                  <div class="d-flex align-items-center">
                    <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                      <i class="bi bi-heart-pulse"></i>
                    </div>
                    <div class="ps-3">
                      <h6 id="currentHeart"></h6>
                      <h6>bpm</h6>
                    </div>
                  </div>
                </div>

              </div>
            </div><!-- End Revenue Card -->

            <!-- Spo2 -->
            <div class="col-xxl-4 col-md-6">
              <div class="card info-card sales-card">

                <div class="card-body">
                  <h5 class="card-title">SpO2</h5>

                  <div class="d-flex align-items-center">
                    <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                      <i class="bi bi-postcard-heart-fill"></i>
                    </div>
                    <div class="ps-3">
                      <h6 id="currentSpo2"></h6>
                      <h6>%</h6>
                    </div>
                  </div>
                </div>

              </div>
            </div>

            <!-- Thân nhiệt -->
            <div class="col-xxl-4 col-md-6">
              <div class="card info-card customers-card">
                <div class="card-body">
                  <h5 class="card-title">Thân Nhiệt</h5>

                  <div class="d-flex align-items-center">
                    <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                      <i class="bi bi-thermometer"></i>
                    </div>
                    <div class="ps-3">
                      <h6 id="currentBodyTemp"></h6>
                      <h6>°C</h6>
                    </div>
                  </div>
                </div>
              </div>

            </div><!-- End Customers Card -->

             <!-- chat luong khong khi -->
            <div class="col-xxl-4 col-md-6">
              <div class="card info-card customers-card">
                <div class="card-body">
                  <h5 class="card-title">Chất lượng không khí</h5>
                  <div class="d-flex align-items-center">
                    <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                      <i class="bi bi-clouds"></i>
                    </div>
                    <div class="ps-3">
                      <h6 id="currentEco2"></h6>
                      <h6>Ppm</h6>
                    </div>
                  </div>
                </div>
              </div>

            </div><!-- End Customers Card -->

            <!-- Reports -->
            <div class="col-12">
              <div class="card">

                <div class="card-body">
                  <h5 class="card-title">Reports</h5>

                  <!-- Line Chart -->
                  <div id="reportsChart"></div>

                  <script type="text/javascript">
                    var chart = Highcharts.chart('reportsChart', {
                      title: {
                        text: 'Đồ thi thông số sức khỏe'
                      },

                      yAxis: {
                        title: {
                          text: 'Number of Employees'
                        }
                      },
                      xAxis: {
                        categories: [],
                        tickWidth: 1,
                        tickLength: 20
                      },
                      legend: {
                        layout: 'vertical',
                        align: 'right',
                        verticalAlign: 'middle'
                      },
                      plotOptions: {
                        series: {
                         label: {
                          connectorAllowed: false
                         },
                        }
                      },
                      series: [{
                       name: 'Nhịp Tim',
                       data: []
                      }, {
                       name: 'SpO2',
                       data: []
                      }, {
                       name: 'Thân Nhiệt',
                       data: []
                      }
                      ],
                      responsive: {
                       rules: [{
                        condition: {
                          maxWidth: 500
                        },
                        chartOptions: {
                         legend: {
                         layout: 'horizontal',
                         align: 'center',
                         verticalAlign: 'bottom'
                        }
                      }
                    }]
                  }

                });

                socket.on("server-send-Heart_graph", function (data) {
                    chart.series[0].setData(data);
                });

                socket.on("server-send-Spo2_graph", function (data) {
                    chart.series[1].setData(data);
                });

                socket.on("server-send-BodyTemp_graph", function (data) {
                    chart.series[2].setData(data);
                });

                socket.on("server-send-date_graph", function (data) {
                    chart.xAxis[0].setCategories(data);
                });
                  </script>
                  <!-- End Line Chart -->

                </div>

              </div>
            </div>

          </div>
        </div>

        <!-- Right side columns -->
        <div class="col-lg-4">

          <!-- Device control -->
          <div style="background-color: rgb(43, 62, 84);cursor: pointer" type="checkbox"
            class="card info-card customers-card"
            >

            <div class="card-body">
              <h5 class="card-title" style="color: white">Device control 1</h5>

